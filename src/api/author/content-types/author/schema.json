{
  "kind": "collectionType",
  "collectionName": "authors",
  "info": {
    "singularName": "author",
    "pluralName": "authors",
    "displayName": "Authors"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true
    },
    "Designation": {
      "type": "string",
      "required": true
    },
    "Image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "Bio": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "Social": {
      "type": "component",
      "component": "shared.social",
      "repeatable": false
    },
    "Slug": {
      "type": "uid",
      "targetField": "Name",
      "required": true
    },
    "anaxdevelopments_coms": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::anaxdevelopment-com.anaxdevelopment-com",
      "mappedBy": "Authors"
    },
    "satishsanpal_coms": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::satishsanpal-com.satishsanpal-com",
      "mappedBy": "Authors"
    },
    "anaxholding_coms": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::anaxholding-com.anaxholding-com",
      "mappedBy": "Authors"
    },
    "SEO": {
      "type": "component",
      "component": "shared.seo",
      "repeatable": false
    },
    "Environment": {
      "type": "enumeration",
      "required": true,
      "default": "Production",
      "enum": [
        "Production",
        "Staging"
      ]
    }
  }
}
